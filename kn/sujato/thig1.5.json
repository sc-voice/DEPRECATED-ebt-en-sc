        {
          "scid": "thig1.5:1.1",
          "pli": "Therīgāthā",
          "en": "Verses of the Senior Nuns"
        },
        {
          "scid": "thig1.5:2.1",
          "pli": "Ekakanipāta",
          "en": "The Book of the Ones"
        },
        {
          "scid": "thig1.5:3.1",
          "pli": "5. Aññatarātissātherīgāthā",
          "en": "1.5. Another Tissā"
        },
        {
          "scid": "thig1.5:4.1",
          "pli": "“Tisse yuñjassu dhammehi,",
          "en": "Tissā, apply yourself to good qualities—"
        },
        {
          "scid": "thig1.5:4.2",
          "pli": "khaṇo taṃ mā upaccagā;",
          "en": "don’t let the moment pass you by."
        },
        {
          "scid": "thig1.5:4.3",
          "pli": "Khaṇātītā hi socanti,",
          "en": "For if you miss your moment,"
        },
        {
          "scid": "thig1.5:4.4",
          "pli": "nirayamhi samappitā”ti.",
          "en": "you’ll grieve when you’re sent to hell."
        },
        {
          "scid": "thig1.5:5.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thig1.5:5.2",
          "pli": "Aññatarā tissā therī ….",
          "en": ""
        }
      ],
