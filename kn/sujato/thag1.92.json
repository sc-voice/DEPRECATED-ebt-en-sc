        {
          "scid": "thag1.92:1.1",
          "pli": "Theragāthā",
          "en": "Verses of the Senior Monks"
        },
        {
          "scid": "thag1.92:2.1",
          "pli": "Ekakanipāta",
          "en": "The Book of the Ones"
        },
        {
          "scid": "thag1.92:3.1",
          "pli": "Dasamavagga",
          "en": "Chapter Ten"
        },
        {
          "scid": "thag1.92:4.1",
          "pli": "1.92. Vijayattheragāthā",
          "en": "1.92. Vijaya"
        },
        {
          "scid": "thag1.92:5.1",
          "pli": "“Yassāsavā parikkhīṇā,",
          "en": "One whose defilements have ended;"
        },
        {
          "scid": "thag1.92:5.2",
          "pli": "āhāre ca anissito;",
          "en": "who’s not attached to food;"
        },
        {
          "scid": "thag1.92:5.3",
          "pli": "Suññato animitto ca,",
          "en": "whose resort is the liberation"
        },
        {
          "scid": "thag1.92:5.4",
          "pli": "vimokkho yassa gocaro;",
          "en": "of the signless and the empty:"
        },
        {
          "scid": "thag1.92:5.5",
          "pli": "Ākāseva sakuntānaṃ,",
          "en": "their track is hard to trace,"
        },
        {
          "scid": "thag1.92:5.6",
          "pli": "padaṃ tassa durannayan”ti.",
          "en": "like birds in the sky."
        },
        {
          "scid": "thag1.92:6.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thag1.92:6.2",
          "pli": "Vijayo thero ….",
          "en": ""
        }
      ],
