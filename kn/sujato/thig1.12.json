        {
          "scid": "thig1.12:3.1",
          "pli": "12. Dhammadinnātherīgāthā",
          "en": "1.12. Dhammadinnā"
        },
        {
          "scid": "thig1.12:4.1",
          "pli": "“Chandajātā avasāyī,",
          "en": "One who is eager and determined"
        },
        {
          "scid": "thig1.12:4.2",
          "pli": "Manasā ca phuṭā siyā;",
          "en": "would be filled with awareness."
        },
        {
          "scid": "thig1.12:4.3",
          "pli": "Kāmesu appaṭibaddhacittā,",
          "en": "One whose mind is not tied up with sensual pleasures"
        },
        {
          "scid": "thig1.12:4.4",
          "pli": "Uddhaṃsotāti vuccatī”ti.",
          "en": "is said to be heading upstream."
        },
        {
          "scid": "thig1.12:5.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thig1.12:5.2",
          "pli": "Dhammadinnā therī ….",
          "en": ""
        }
      ],
