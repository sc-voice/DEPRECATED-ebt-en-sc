      "segments": [
        {
          "scid": "thig1.11:1.1",
          "pli": "Therīgāthā",
          "en": "Verses of the Senior Nuns"
        },
        {
          "scid": "thig1.11:2.1",
          "pli": "Ekakanipāta",
          "en": "The Book of the Ones"
        },
        {
          "scid": "thig1.11:3.1",
          "pli": "11. Muttātherīgāthā",
          "en": "1.11. Muttā (2nd)"
        },
        {
          "scid": "thig1.11:4.1",
          "pli": "“Sumuttā sādhumuttāmhi,",
          "en": "I’m well freed, so very well freed,"
        },
        {
          "scid": "thig1.11:4.2",
          "pli": "tīhi khujjehi muttiyā;",
          "en": "freed from the three things that bent me over:"
        },
        {
          "scid": "thig1.11:4.3",
          "pli": "Udukkhalena musalena,",
          "en": "the mortar, the pestle,"
        },
        {
          "scid": "thig1.11:4.4",
          "pli": "patinā khujjakena ca;",
          "en": "and my humpbacked husband."
        },
        {
          "scid": "thig1.11:4.5",
          "pli": "Muttāmhi jātimaraṇā,",
          "en": "I’m freed from birth and death;"
        },
        {
          "scid": "thig1.11:4.6",
          "pli": "bhavanetti samūhatā”ti.",
          "en": "the attachment to rebirth is eradicated."
        },
        {
          "scid": "thig1.11:5.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thig1.11:5.2",
          "pli": "Muttā therī ….",
          "en": ""
        }
      ],
