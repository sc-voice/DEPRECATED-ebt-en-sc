        {
          "scid": "thag1.61:4.1",
          "pli": "1.61. Vappattheragāthā",
          "en": "1.61. Vappa"
        },
        {
          "scid": "thag1.61:5.1",
          "pli": "“Passati passo passantaṃ,",
          "en": "One who sees"
        },
        {
          "scid": "thag1.61:5.2",
          "pli": "apassantañca passati;",
          "en": "sees those who see and those who don’t."
        },
        {
          "scid": "thag1.61:5.3",
          "pli": "Apassanto apassantaṃ,",
          "en": "One who doesn’t see"
        },
        {
          "scid": "thag1.61:5.4",
          "pli": "passantañca na passatī”ti.",
          "en": "sees neither."
        },
        {
          "scid": "thag1.61:6.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thag1.61:6.2",
          "pli": "Vappo thero ….",
          "en": ""
        }
      ],
