        },
        {
          "scid": "thag1.69:4.1",
          "pli": "1.69. Channattheragāthā",
          "en": "1.69. Channa"
        },
        {
          "scid": "thag1.69:5.1",
          "pli": "“Sutvāna dhammaṃ mahato mahārasaṃ,",
          "en": "Hearing the sweet Dhamma taught by the master"
        },
        {
          "scid": "thag1.69:5.2",
          "pli": "Sabbaññutaññāṇavarena desitaṃ;",
          "en": "of universal understanding and superior knowledge,"
        },
        {
          "scid": "thag1.69:5.3",
          "pli": "Maggaṃ papajjiṃ amatassa pattiyā,",
          "en": "I’ve entered the path to realize the deathless—"
        },
        {
          "scid": "thag1.69:5.4",
          "pli": "So yogakkhemassa pathassa kovido”ti.",
          "en": "he is the expert on the road to sanctuary."
        },
        {
          "scid": "thag1.69:6.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thag1.69:6.2",
          "pli": "Channo thero ….",
          "en": ""
        }
      ],
