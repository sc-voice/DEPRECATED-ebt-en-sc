      "segments": [
        {
          "scid": "thag1.90:1.1",
          "pli": "Theragāthā",
          "en": "Verses of the Senior Monks"
        },
        {
          "scid": "thag1.90:2.1",
          "pli": "Ekakanipāta",
          "en": "The Book of the Ones"
        },
        {
          "scid": "thag1.90:3.1",
          "pli": "Navamavagga",
          "en": "Chapter Nine"
        },
        {
          "scid": "thag1.90:4.1",
          "pli": "1.90. Sāmidattattheragāthā",
          "en": "1.90. Sāmidatta"
        },
        {
          "scid": "thag1.90:5.1",
          "pli": "“Pañcakkhandhā pariññātā,",
          "en": "The five aggregates are fully understood;"
        },
        {
          "scid": "thag1.90:5.2",
          "pli": "tiṭṭhanti chinnamūlakā;",
          "en": "they remain, but their root is cut."
        },
        {
          "scid": "thag1.90:5.3",
          "pli": "Vikkhīṇo jātisaṃsāro,",
          "en": "Transmigration is finished,"
        },
        {
          "scid": "thag1.90:5.4",
          "pli": "natthi dāni punabbhavo”ti.",
          "en": "now there are no more future lives."
        },
        {
          "scid": "thag1.90:6.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thag1.90:6.2",
          "pli": "Sāmidatto thero ….",
          "en": ""
        },
        {
          "scid": "thag1.90:6.3",
          "pli": "Vaggo navamo.",
          "en": ""
        },
        {
          "scid": "thag1.90:7.1",
          "pli": "Tassuddānaṃ",
          "en": ""
        },
        {
          "scid": "thag1.90:8.1",
          "pli": "Thero samitigutto ca,",
          "en": ""
        },
        {
          "scid": "thag1.90:8.2",
          "pli": "kassapo sīhasavhayo;",
          "en": ""
        },
        {
          "scid": "thag1.90:8.3",
          "pli": "Nīto sunāgo nāgito,",
          "en": ""
        },
        {
          "scid": "thag1.90:8.4",
          "pli": "paviṭṭho ajjuno isi;",
          "en": ""
        },
        {
          "scid": "thag1.90:8.5",
          "pli": "Devasabho ca yo thero,",
          "en": ""
        },
        {
          "scid": "thag1.90:8.6",
          "pli": "sāmidatto mahabbaloti.",
          "en": ""
        }
      ],
