          "scid": "thag1.91:4.1",
          "pli": "1.91. Paripuṇṇakattheragāthā",
          "en": "1.91. Paripuṇṇaka"
        },
        {
          "scid": "thag1.91:5.1",
          "pli": "“Na tathā mataṃ satarasaṃ,",
          "en": "What I consumed today is considered better"
        },
        {
          "scid": "thag1.91:5.2",
          "pli": "Sudhannaṃ yaṃ mayajja paribhuttaṃ;",
          "en": "than delicious grain of a hundred flavors—"
        },
        {
          "scid": "thag1.91:5.3",
          "pli": "Aparimitadassinā gotamena,",
          "en": "the Dhamma taught by the Buddha,"
        },
        {
          "scid": "thag1.91:5.4",
          "pli": "Buddhena desito dhammo”ti.",
          "en": "Gotama of infinite vision."
        },
        {
          "scid": "thag1.91:6.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thag1.91:6.2",
          "pli": "Paripuṇṇako thero ….",
          "en": ""
        }
