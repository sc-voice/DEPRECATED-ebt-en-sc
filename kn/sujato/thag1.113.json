          "scid": "thag1.113:4.1",
          "pli": "1.113. Vanavacchattheragāthā",
          "en": "1.113. Vanavaccha (2nd)"
        },
        {
          "scid": "thag1.113:5.1",
          "pli": "“Acchodikā puthusilā,",
          "en": "The water’s clear and the rocks are broad,"
        },
        {
          "scid": "thag1.113:5.2",
          "pli": "gonaṅgulamigāyutā;",
          "en": "monkeys and deer are all around;"
        },
        {
          "scid": "thag1.113:5.3",
          "pli": "Ambusevālasañchannā,",
          "en": "festooned with dewy moss,"
        },
        {
          "scid": "thag1.113:5.4",
          "pli": "te selā ramayanti man”ti.",
          "en": "these rocky crags delight me!"
        },
        {
          "scid": "thag1.113:6.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thag1.113:6.2",
          "pli": "Vanavaccho thero ….",
          "en": ""
        }
      ],
