        {
          "scid": "thag1.89:4.1",
          "pli": "1.89. (Paṭhama) Devasabhattheragāthā",
          "en": "1.89. Devasabha (1st)"
        },
        {
          "scid": "thag1.89:5.1",
          "pli": "“Uttiṇṇā paṅkapalipā,",
          "en": "I’ve crossed the bogs,"
        },
        {
          "scid": "thag1.89:5.2",
          "pli": "pātālā parivajjitā;",
          "en": "I’ve avoided the cliffs,"
        },
        {
          "scid": "thag1.89:5.3",
          "pli": "Mutto oghā ca ganthā ca,",
          "en": "I’m freed from floods and ties,"
        },
        {
          "scid": "thag1.89:5.4",
          "pli": "sabbe mānā visaṃhatā”ti.",
          "en": "and I've wiped out all conceit."
        },
        {
          "scid": "thag1.89:6.1",
          "pli": "…",
          "en": ""
        },
        {
          "scid": "thag1.89:6.2",
          "pli": "Devasabho thero ….",
          "en": ""
        }
      ],
