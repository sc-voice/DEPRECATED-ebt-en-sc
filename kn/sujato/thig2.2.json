      "segments": [
        {
          "scid": "thig2.2:1.1",
          "pli": "Therīgāthā",
          "en": "Verses of the Senior Nuns"
        },
        {
          "scid": "thig2.2:2.1",
          "pli": "Dukanipāta",
          "en": "The Book of the Twos"
        },
        {
          "scid": "thig2.2:3.1",
          "pli": "2. Jentātherīgāthā",
          "en": "2.2. Jentā"
        },
        {
          "scid": "thig2.2:4.1",
          "pli": "“Ye ime satta bojjhaṅgā,",
          "en": "Of the seven awakening factors,"
        },
        {
          "scid": "thig2.2:4.2",
          "pli": "maggā nibbānapattiyā;",
          "en": "the path for attaining extinguishment,"
        },
        {
          "scid": "thig2.2:4.3",
          "pli": "Bhāvitā te mayā sabbe,",
          "en": "I have developed them all,"
        },
        {
          "scid": "thig2.2:4.4",
          "pli": "yathā buddhena desitā.",
          "en": "just as the Buddha taught."
        },
        {
          "scid": "thig2.2:5.1",
          "pli": "Diṭṭho hi me so bhagavā,",
          "en": "For I have seen the Blessed One,"
        },
        {
          "scid": "thig2.2:5.2",
          "pli": "antimoyaṃ samussayo;",
          "en": "and this bag of bones is my last."
        },
        {
          "scid": "thig2.2:5.3",
          "pli": "Vikkhīṇo jātisaṃsāro,",
          "en": "Transmigration through births is finished,"
        },
        {
          "scid": "thig2.2:5.4",
          "pli": "natthi dāni punabbhavo”ti.",
          "en": "now there are no more future lives."
        },
        {
          "scid": "thig2.2:6.1",
          "pli": "Itthaṃ sudaṃ jentā therī gāthāyo abhāsitthāti.",
          "en": "It was thus that these verses were recited by the senior nun Jentā."
        }
      ],
