    {
      "segments": [
        {
          "scid": "sn22.34:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn22.34:1.2",
          "pli": "“Yaṃ, bhikkhave, na tumhākaṃ, taṃ pajahatha. ",
          "en": "“Mendicants, give up what’s not yours. "
        },
        {
          "scid": "sn22.34:1.3",
          "pli": "Taṃ vo pahīnaṃ hitāya sukhāya bhavissati. ",
          "en": "Giving it up will be for your welfare and happiness. "
        },
        {
          "scid": "sn22.34:1.4",
          "pli": "Kiñca, bhikkhave, na tumhākaṃ? ",
          "en": "And what isn’t yours? "
        },
        {
          "scid": "sn22.34:1.5",
          "pli": "Rūpaṃ, bhikkhave, na tumhākaṃ, taṃ pajahatha. ",
          "en": "Form isn’t yours: give it up. "
        },
        {
          "scid": "sn22.34:1.6",
          "pli": "Taṃ vo pahīnaṃ hitāya sukhāya bhavissati. ",
          "en": "Giving it up will be for your welfare and happiness. "
        },
        {
          "scid": "sn22.34:1.7",
          "pli": "Vedanā na tumhākaṃ … ",
          "en": "Feeling … "
        },
        {
          "scid": "sn22.34:1.8",
          "pli": "saññā na tumhākaṃ … ",
          "en": "Perception … "
        },
        {
          "scid": "sn22.34:1.9",
          "pli": "saṅkhārā na tumhākaṃ … ",
          "en": "Choices … "
        },
        {
          "scid": "sn22.34:1.10",
          "pli": "viññāṇaṃ na tumhākaṃ, taṃ pajahatha. ",
          "en": "Consciousness isn’t yours: give it up. "
        },
        {
          "scid": "sn22.34:1.11",
          "pli": "Taṃ vo pahīnaṃ hitāya sukhāya bhavissati. ",
          "en": "Giving it up will be for your welfare and happiness. "
        },
        {
          "scid": "sn22.34:1.12",
          "pli": "Yaṃ, bhikkhave, na tumhākaṃ taṃ pajahatha. ",
          "en": "Give up what’s not yours. "
        },
        {
          "scid": "sn22.34:1.13",
          "pli": "Taṃ vo pahīnaṃ hitāya sukhāya bhavissatī”ti. ",
          "en": "Giving it up will be for your welfare and happiness.” "
        },
        {
          "scid": "sn22.34:1.14",
          "pli": "Dutiyaṃ. ",
          "en": ""
        }
      ],
