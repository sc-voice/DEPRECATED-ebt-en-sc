      "segments": [
        {
          "scid": "sn26.3:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn26.3:1.2",
          "pli": "“Yo kho, bhikkhave, cakkhuviññāṇassa uppādo ṭhiti … pe … ",
          "en": "“Mendicants, the arising of eye consciousness … "
        },
        {
          "scid": "sn26.3:1.3",
          "pli": "jarāmaraṇassa pātubhāvo … pe … ",
          "en": " "
        },
        {
          "scid": "sn26.3:1.4",
          "pli": "yo manoviññāṇassa uppādo ṭhiti … pe … ",
          "en": "mind consciousness … "
        },
        {
          "scid": "sn26.3:1.5",
          "pli": "jarāmaraṇassa pātubhāvo. ",
          "en": "is the manifestation of old age and death. "
        },
        {
          "scid": "sn26.3:1.6",
          "pli": "Yo ca kho, bhikkhave, cakkhuviññāṇassa nirodho … pe … ",
          "en": "The cessation of eye consciousness … "
        },
        {
          "scid": "sn26.3:1.7",
          "pli": "jarāmaraṇassa atthaṅgamo … pe … ",
          "en": " "
        },
        {
          "scid": "sn26.3:1.8",
          "pli": "yo manoviññāṇassa nirodho … pe … ",
          "en": "mind consciousness … "
        },
        {
          "scid": "sn26.3:1.9",
          "pli": "jarāmaraṇassa atthaṅgamo”ti. ",
          "en": "is the ending of old age and death.” "
        },
        {
          "scid": "sn26.3:1.10",
          "pli": "Tatiyaṃ. ",
          "en": ""
        }
      ],
