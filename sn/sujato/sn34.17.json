    {
      "segments": [
        {
          "scid": "sn34.17:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn34.17:1.2",
          "pli": "“Cattārome, bhikkhave, jhāyī. ",
          "en": "“Mendicants, there are these four meditators. "
        },
        {
          "scid": "sn34.17:1.3",
          "pli": "Katame cattāro? ",
          "en": "What four? "
        },
        {
          "scid": "sn34.17:1.4",
          "pli": "Idha, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo hoti, na samādhismiṃ sakkaccakārī. ",
          "en": "One meditator is skilled in entering immersion but not in practicing carefully for it. …” "
        },
        {
          "scid": "sn34.17:1.5",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ sakkaccakārī hoti, na samādhismiṃ samāpattikusalo. ",
          "en": " "
        },
        {
          "scid": "sn34.17:1.6",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī neva samādhismiṃ samāpattikusalo hoti, na ca samādhismiṃ sakkaccakārī. ",
          "en": " "
        },
        {
          "scid": "sn34.17:1.7",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo ca hoti, samādhismiṃ sakkaccakārī ca. ",
          "en": " "
        },
        {
          "scid": "sn34.17:1.8",
          "pli": "Tatra … pe … ",
          "en": " "
        },
        {
          "scid": "sn34.17:1.9",
          "pli": "pavaro cā”ti. ",
          "en": " "
        },
        {
          "scid": "sn34.17:1.10",
          "pli": "Sattarasamaṃ. ",
          "en": ""
        }
      ],
