    {
      "segments": [
        {
          "scid": "sn18.5:1.1",
          "pli": "Sāvatthiyaṃ viharati. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn18.5:1.2",
          "pli": "“Taṃ kiṃ maññasi, rāhula, ",
          "en": "“What do you think, Rāhula? "
        },
        {
          "scid": "sn18.5:1.3",
          "pli": "cakkhusamphassajā vedanā niccā vā aniccā vā”ti? ",
          "en": "Is feeling born of eye contact permanent or impermanent?” "
        },
        {
          "scid": "sn18.5:1.4",
          "pli": "“Aniccā, bhante” … ",
          "en": "“Impermanent, sir.” … "
        },
        {
          "scid": "sn18.5:1.5",
          "pli": "“sotasamphassajā vedanā … pe … ",
          "en": "“… feeling born of ear contact … "
        },
        {
          "scid": "sn18.5:1.6",
          "pli": "ghānasamphassajā vedanā … ",
          "en": "feeling born of nose contact … "
        },
        {
          "scid": "sn18.5:1.7",
          "pli": "jivhāsamphassajā vedanā … ",
          "en": "feeling born of tongue contact … "
        },
        {
          "scid": "sn18.5:1.8",
          "pli": "kāyasamphassajā vedanā … ",
          "en": "feeling born of body contact … "
        },
        {
          "scid": "sn18.5:1.9",
          "pli": "manosamphassajā vedanā niccā vā aniccā vā”ti? ",
          "en": "Is feeling born of mind contact permanent or impermanent?” "
        },
        {
          "scid": "sn18.5:1.10",
          "pli": "“Aniccā, bhante” … ",
          "en": "“Impermanent, sir.” … "
        },
        {
          "scid": "sn18.5:1.11",
          "pli": "“evaṃ passaṃ, rāhula, sutavā ariyasāvako cakkhusamphassajāya vedanāyapi nibbindati … pe … sota … ghāna … jivhā … kāya … manosamphassajāya vedanāyapi nibbindati … pe … ",
          "en": "“Seeing this, a learned noble disciple grows disillusioned with feeling born of eye contact, ear contact, nose contact, tongue contact, body contact, and mind contact. …” "
        },
        {
          "scid": "sn18.5:1.12",
          "pli": "pajānātī”ti. ",
          "en": " "
        },
        {
          "scid": "sn18.5:1.13",
          "pli": "Pañcamaṃ. ",
          "en": ""
        }
      ],
