        {
          "scid": "sn22.143:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn22.143:1.2",
          "pli": "“Yo, bhikkhave, anattā; tatra vo chando pahātabbo. ",
          "en": "“Mendicants, you should give up desire for what is not-self. …” "
        },
        {
          "scid": "sn22.143:1.3",
          "pli": "Ko ca, bhikkhave, anattā? ",
          "en": " "
        },
        {
          "scid": "sn22.143:1.4",
          "pli": "Rūpaṃ, bhikkhave, anattā; tatra vo chando pahātabbo. ",
          "en": " "
        },
        {
          "scid": "sn22.143:1.5",
          "pli": "Vedanā anattā … ",
          "en": " "
        },
        {
          "scid": "sn22.143:1.6",
          "pli": "saññā … ",
          "en": " "
        },
        {
          "scid": "sn22.143:1.7",
          "pli": "saṅkhārā … ",
          "en": " "
        },
        {
          "scid": "sn22.143:1.8",
          "pli": "viññāṇaṃ anattā; tatra vo chando pahātabbo. ",
          "en": " "
        },
        {
          "scid": "sn22.143:1.9",
          "pli": "Yo, bhikkhave, anattā; tatra vo chando pahātabbo”ti. ",
          "en": " "
        },
        {
          "scid": "sn22.143:1.10",
          "pli": "Aṭṭhamaṃ. ",
          "en": ""
        }
      ],
