    {
      "segments": [
        {
          "scid": "sn1.64:1.1",
          "pli": "“Kiṃsu saṃyojano loko, ",
          "en": "“What fetters the world? "
        },
        {
          "scid": "sn1.64:1.2",
          "pli": "kiṃsu tassa vicāraṇaṃ; ",
          "en": "What explores it? "
        },
        {
          "scid": "sn1.64:1.3",
          "pli": "Kissassu vippahānena, ",
          "en": "With the giving up of what "
        },
        {
          "scid": "sn1.64:1.4",
          "pli": "nibbānaṃ iti vuccatī”ti. ",
          "en": "is extinguishment spoken of?” "
        },
        {
          "scid": "sn1.64:2.1",
          "pli": "“Nandīsaṃyojano loko, ",
          "en": "“Delight fetters the world. "
        },
        {
          "scid": "sn1.64:2.2",
          "pli": "vitakkassa vicāraṇaṃ; ",
          "en": "Thought explores it. "
        },
        {
          "scid": "sn1.64:2.3",
          "pli": "Taṇhāya vippahānena, ",
          "en": "With the giving up of craving "
        },
        {
          "scid": "sn1.64:2.4",
          "pli": "nibbānaṃ iti vuccatī”ti. ",
          "en": "extinguishment is spoken of.”"
        }
      ],
