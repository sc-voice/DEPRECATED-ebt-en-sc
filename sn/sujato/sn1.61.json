    {
      "segments": [
        {
          "scid": "sn1.61:1.1",
          "pli": "“Kiṃsu sabbaṃ addhabhavi, ",
          "en": "“What oppresses everything? "
        },
        {
          "scid": "sn1.61:1.2",
          "pli": "kismā bhiyyo na vijjati; ",
          "en": "What is nothing bigger than? "
        },
        {
          "scid": "sn1.61:1.3",
          "pli": "Kissassu ekadhammassa, ",
          "en": "What is the one thing "
        },
        {
          "scid": "sn1.61:1.4",
          "pli": "sabbeva vasamanvagū”ti. ",
          "en": "that has everything under its sway?” "
        },
        {
          "scid": "sn1.61:2.1",
          "pli": "“Nāmaṃ sabbaṃ addhabhavi, ",
          "en": "“Name oppresses everything. "
        },
        {
          "scid": "sn1.61:2.2",
          "pli": "nāmā bhiyyo na vijjati; ",
          "en": "Nothing’s bigger than name. "
        },
        {
          "scid": "sn1.61:2.3",
          "pli": "Nāmassa ekadhammassa, ",
          "en": "Name is the one thing "
        },
        {
          "scid": "sn1.61:2.4",
          "pli": "sabbeva vasamanvagū”ti. ",
          "en": "that has everything under its sway.”"
        }
      ],
