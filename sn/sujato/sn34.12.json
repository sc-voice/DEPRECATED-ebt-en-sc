    {
      "segments": [
        {
          "scid": "sn34.12:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn34.12:1.2",
          "pli": "“Cattārome, bhikkhave, jhāyī. ",
          "en": "“Mendicants, there are these four meditators. "
        },
        {
          "scid": "sn34.12:1.3",
          "pli": "Katame cattāro? ",
          "en": "What four? "
        },
        {
          "scid": "sn34.12:1.4",
          "pli": "Idha, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo hoti, na samādhismiṃ vuṭṭhānakusalo. ",
          "en": "One meditator is skilled in entering immersion but not in emerging from it. …” "
        },
        {
          "scid": "sn34.12:1.5",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ vuṭṭhānakusalo hoti, na samādhismiṃ samāpattikusalo. ",
          "en": " "
        },
        {
          "scid": "sn34.12:1.6",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī neva samādhismiṃ samāpattikusalo hoti, na ca samādhismiṃ vuṭṭhānakusalo. ",
          "en": " "
        },
        {
          "scid": "sn34.12:1.7",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo ca hoti, samādhismiṃ vuṭṭhānakusalo ca. ",
          "en": " "
        },
        {
          "scid": "sn34.12:1.8",
          "pli": "Tatra, bhikkhave, yvāyaṃ jhāyī … pe … ",
          "en": " "
        },
        {
          "scid": "sn34.12:1.9",
          "pli": "pavaro cā”ti. ",
          "en": " "
        },
        {
          "scid": "sn34.12:1.10",
          "pli": "Dvādasamaṃ. ",
          "en": ""
        }
      ],
