    },
    {
      "segments": [
        {
          "scid": "sn12.72-81:1.1",
          "pli": "“Sāvatthiyaṃ viharati. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn12.72-81:1.2",
          "pli": "Jātiṃ nappajānanti … pe …. ",
          "en": "“… they don’t understand rebirth … "
        },
        {
          "scid": "sn12.72-81:2.1",
          "pli": "“Bhavaṃ nappajānanti … pe …. ",
          "en": "continued existence … "
        },
        {
          "scid": "sn12.72-81:3.1",
          "pli": "“Upādānaṃ nappajānanti … pe …. ",
          "en": "grasping … "
        },
        {
          "scid": "sn12.72-81:4.1",
          "pli": "“Taṇhaṃ nappajānanti … pe …. ",
          "en": "craving … "
        },
        {
          "scid": "sn12.72-81:5.1",
          "pli": "“Vedanaṃ nappajānanti … pe …. ",
          "en": "feeling … "
        },
        {
          "scid": "sn12.72-81:6.1",
          "pli": "“Phassaṃ nappajānanti … pe …. ",
          "en": "contact … "
        },
        {
          "scid": "sn12.72-81:7.1",
          "pli": "“Saḷāyatanaṃ nappajānanti … pe …. ",
          "en": "the six sense fields … "
        },
        {
          "scid": "sn12.72-81:8.1",
          "pli": "“Nāmarūpaṃ nappajānanti … pe …. ",
          "en": "name and form … "
        },
        {
          "scid": "sn12.72-81:9.1",
          "pli": "“Viññāṇaṃ nappajānanti … pe …. ",
          "en": "consciousness … "
        },
        {
          "scid": "sn12.72-81:10.1",
          "pli": "“Saṅkhāre nappajānanti, saṅkhārasamudayaṃ nappajānanti, saṅkhāranirodhaṃ nappajānanti, saṅkhāranirodhagāminiṃ paṭipadaṃ nappajānanti … pe … ",
          "en": "choices … "
        },
        {
          "scid": "sn12.72-81:10.2",
          "pli": "pajānanti … pe … ",
          "en": "… they understand …” "
        },
        {
          "scid": "sn12.72-81:10.3",
          "pli": "sayaṃ abhiññā sacchikatvā upasampajja viharantī”ti. ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:10.4",
          "pli": "Ekādasamaṃ. ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:10.5",
          "pli": "Samaṇabrāhmaṇavaggo aṭṭhamo. ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:11.1",
          "pli": "Paccayekādasa vuttā, ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:11.2",
          "pli": "catusaccavibhajjanā; ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:11.3",
          "pli": "Samaṇabrāhmaṇavaggo, ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:11.4",
          "pli": "nidāne bhavati aṭṭhamo. ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:12.1",
          "pli": "Buddho āhāro dasabalo, ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:12.2",
          "pli": "Kaḷāro gahapatipañcamo; ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:12.3",
          "pli": "Dukkhavaggo mahāvaggo, ",
          "en": " "
        },
        {
          "scid": "sn12.72-81:12.4",
          "pli": "Aṭṭhamo samaṇabrāhmaṇoti. ",
          "en": ""
        }
      ],
