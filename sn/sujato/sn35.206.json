    {
      "segments": [
        {
          "scid": "sn35.206:1.1",
          "pli": "“Cakkhu, bhikkhave, aniccaṃ paccuppannaṃ. ",
          "en": "“Mendicants, in the present the eye, ear, nose, tongue, body, and mind are impermanent. "
        },
        {
          "scid": "sn35.206:1.2",
          "pli": "Yadaniccaṃ, taṃ dukkhaṃ. ",
          "en": "What’s impermanent is suffering. …” "
        },
        {
          "scid": "sn35.206:1.3",
          "pli": "Yaṃ dukkhaṃ, tadanattā. ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.4",
          "pli": "Yadanattā taṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ yathābhūtaṃ sammappaññāya daṭṭhabbaṃ … pe … ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.5",
          "pli": "jivhā aniccā paccuppannā. ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.6",
          "pli": "Yadaniccaṃ, taṃ dukkhaṃ. ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.7",
          "pli": "Yaṃ dukkhaṃ, tadanattā. ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.8",
          "pli": "Yadanattā, taṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ yathābhūtaṃ sammappaññāya daṭṭhabbaṃ … pe … ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.9",
          "pli": "mano anicco paccuppanno. ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.10",
          "pli": "Yadaniccaṃ taṃ dukkhaṃ. ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.11",
          "pli": "Yaṃ dukkhaṃ tadanattā. ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.12",
          "pli": "Yadanattā, taṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ yathābhūtaṃ sammappaññāya daṭṭhabbaṃ. ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.13",
          "pli": "Evaṃ passaṃ … pe … ",
          "en": " "
        },
        {
          "scid": "sn35.206:1.14",
          "pli": "nāparaṃ itthattāyāti pajānātī”ti. ",
          "en": ""
        }
      ],
