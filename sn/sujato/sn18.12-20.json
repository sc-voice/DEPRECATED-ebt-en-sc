          "pli": "jivhāsamphasso … ",
          "en": "tongue contact … "
        },
        {
          "scid": "sn18.12-20:3.5",
          "pli": "kāyasamphasso … ",
          "en": "body contact … "
        },
        {
          "scid": "sn18.12-20:3.6",
          "pli": "manosamphasso …. ",
          "en": "mind contact …” "
        },
        {
          "scid": "sn18.12-20:4.1",
          "pli": "Cakkhusamphassajā vedanā … pe … ",
          "en": "“… feeling born of eye contact … "
        },
        {
          "scid": "sn18.12-20:4.2",
          "pli": "sotasamphassajā vedanā … ",
          "en": "feeling born of ear contact … "
        },
        {
          "scid": "sn18.12-20:4.3",
          "pli": "ghānasamphassajā vedanā … ",
          "en": "feeling born of nose contact … "
        },
        {
          "scid": "sn18.12-20:4.4",
          "pli": "jivhāsamphassajā vedanā … ",
          "en": "feeling born of tongue contact … "
        },
        {
          "scid": "sn18.12-20:4.5",
          "pli": "kāyasamphassajā vedanā … ",
          "en": "feeling born of body contact … "
        },
        {
          "scid": "sn18.12-20:4.6",
          "pli": "manosamphassajā vedanā …. ",
          "en": "feeling born of mind contact …” "
        },
        {
          "scid": "sn18.12-20:5.1",
          "pli": "Rūpasaññā … pe … ",
          "en": "“… perception of sights … "
        },
        {
          "scid": "sn18.12-20:5.2",
