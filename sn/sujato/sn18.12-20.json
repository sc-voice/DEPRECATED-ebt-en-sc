        {
          "scid": "sn18.12-20:1.1",
          "pli": "Sāvatthiyaṃ viharati. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn18.12-20:1.2",
          "pli": "“Taṃ kiṃ maññasi, rāhula, ",
          "en": "“What do you think, Rāhula? "
        },
        {
          "scid": "sn18.12-20:1.3",
          "pli": "rūpā niccā vā aniccā vā”ti? ",
          "en": "Are sights permanent or impermanent?” "
        },
        {
          "scid": "sn18.12-20:1.4",
          "pli": "“Aniccā, bhante” … pe … ",
          "en": "“Impermanent, sir.” … "
        },
        {
          "scid": "sn18.12-20:1.5",
          "pli": "saddā … ",
          "en": "“… sounds … "
        },
        {
          "scid": "sn18.12-20:1.6",
          "pli": "gandhā … ",
          "en": "smells … "
        },
        {
          "scid": "sn18.12-20:1.7",
          "pli": "rasā … ",
          "en": "tastes … "
        },
        {
          "scid": "sn18.12-20:1.8",
          "pli": "phoṭṭhabbā … ",
          "en": "touches … "
        },
        {
          "scid": "sn18.12-20:1.9",
          "pli": "dhammā …. ",
          "en": "thoughts …” "
        },
        {
          "scid": "sn18.12-20:2.1",
          "pli": "“Cakkhuviññāṇaṃ … pe … ",
          "en": "“… eye consciousness … "
        },
        {
          "scid": "sn18.12-20:2.2",
          "pli": "sotaviññāṇaṃ … ",
          "en": "ear consciousness … "
        },
        {
          "scid": "sn18.12-20:2.3",
          "pli": "ghānaviññāṇaṃ … ",
          "en": "nose consciousness … "
        },
        {
          "scid": "sn18.12-20:2.4",
          "pli": "jivhāviññāṇaṃ … ",
          "en": "tongue consciousness … "
        },
        {
          "scid": "sn18.12-20:2.5",
          "pli": "kāyaviññāṇaṃ … ",
          "en": "body consciousness … "
        },
        {
          "scid": "sn18.12-20:2.6",
          "pli": "manoviññāṇaṃ …. ",
          "en": "mind consciousness …” "
        },
        {
          "scid": "sn18.12-20:3.1",
          "pli": "Cakkhusamphasso … pe … ",
          "en": "“… eye contact … "
        },
        {
          "scid": "sn18.12-20:3.2",
          "pli": "sotasamphasso … ",
          "en": "ear contact … "
        },
        {
          "scid": "sn18.12-20:3.3",
          "pli": "ghānasamphasso … ",
          "en": "nose contact … "
        },
        {
