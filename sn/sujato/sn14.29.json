    {
      "segments": [
        {
          "scid": "sn14.29:1.1",
          "pli": "Sāvatthiyaṃ viharati. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn14.29:1.2",
          "pli": "“Dhātusova, bhikkhave, sattā saṃsandanti samenti. ",
          "en": "“Mendicants, sentient beings come together and converge because of an element: "
        },
        {
          "scid": "sn14.29:1.3",
          "pli": "Micchādiṭṭhikā micchādiṭṭhikehi saddhiṃ saṃsandanti samenti; ",
          "en": "those of wrong view with those of wrong view … "
        },
        {
          "scid": "sn14.29:1.4",
          "pli": "micchāsaṅkappā … pe … ",
          "en": "wrong thought … "
        },
        {
          "scid": "sn14.29:1.5",
          "pli": "micchāvācā … ",
          "en": "wrong speech … "
        },
        {
          "scid": "sn14.29:1.6",
          "pli": "micchākammantā … ",
          "en": "wrong action … "
        },
        {
          "scid": "sn14.29:1.7",
          "pli": "micchāājīvā … ",
          "en": "wrong livelihood … "
        },
        {
          "scid": "sn14.29:1.8",
          "pli": "micchāvāyāmā … ",
          "en": "wrong effort … "
        },
        {
          "scid": "sn14.29:1.9",
          "pli": "micchāsatino … ",
          "en": "wrong mindfulness … "
        },
        {
          "scid": "sn14.29:1.10",
          "pli": "micchāsamādhino micchāsamādhīhi saddhiṃ saṃsandanti samenti; ",
          "en": "wrong immersion … "
        },
        {
          "scid": "sn14.29:1.11",
          "pli": "micchāñāṇino micchāñāṇīhi saddhiṃ saṃsandanti samenti; ",
          "en": "wrong knowledge … "
        },
        {
          "scid": "sn14.29:1.12",
          "pli": "micchāvimuttino micchāvimuttīhi saddhiṃ saṃsandanti samenti. ",
          "en": "wrong freedom … "
        },
        {
          "scid": "sn14.29:2.1",
          "pli": "Sammādiṭṭhikā sammādiṭṭhikehi saddhiṃ saṃsandanti samenti; ",
          "en": "Those who have right view … "
        },
        {
          "scid": "sn14.29:2.2",
          "pli": "sammāsaṅkappā … pe … ",
          "en": "right thought … "
        },
        {
          "scid": "sn14.29:2.3",
          "pli": "sammāvācā … ",
          "en": "right speech … "
        },
        {
          "scid": "sn14.29:2.4",
          "pli": "sammākammantā … ",
          "en": "right action … "
        },
        {
          "scid": "sn14.29:2.5",
          "pli": "sammāājīvā … ",
          "en": "right livelihood … "
        },
        {
          "scid": "sn14.29:2.6",
          "pli": "sammāvāyāmā … ",
          "en": "right effort … "
        },
        {
          "scid": "sn14.29:2.7",
          "pli": "sammāsatino … ",
          "en": "right mindfulness … "
        },
        {
          "scid": "sn14.29:2.8",
          "pli": "sammāsamādhino … ",
          "en": "right immersion … "
        },
        {
          "scid": "sn14.29:2.9",
          "pli": "sammāñāṇino sammāñāṇīhi saddhiṃ saṃsandanti samenti; ",
          "en": "right knowledge … "
        },
        {
          "scid": "sn14.29:2.10",
          "pli": "sammāvimuttino sammāvimuttīhi saddhiṃ saṃsandanti samentī”ti. ",
          "en": "right freedom with those who have right freedom.” "
        },
        {
          "scid": "sn14.29:2.11",
          "pli": "Sattamaṃ. ",
          "en": " "
        },
        {
          "scid": "sn14.29:2.12",
          "pli": "(Sabbattha atītānāgatapaccuppannaṃ kātabbaṃ.) ",
          "en": "(All these should be treated in terms of the past, future, and present.) "
        },
        {
          "scid": "sn14.29:3.1",
          "pli": "Asamāhitaṃ dussīlaṃ, ",
          "en": " "
        },
        {
          "scid": "sn14.29:3.2",
          "pli": "pañca sikkhāpadāni ca; ",
          "en": " "
        },
        {
          "scid": "sn14.29:3.3",
          "pli": "Satta kammapathā vuttā, ",
          "en": " "
        },
        {
          "scid": "sn14.29:3.4",
          "pli": "dasakammapathena ca; ",
          "en": " "
        },
        {
          "scid": "sn14.29:3.5",
          "pli": "Chaṭṭhaṃ aṭṭhaṅgiko vutto, ",
          "en": " "
        },
        {
          "scid": "sn14.29:3.6",
          "pli": "dasaṅgena ca sattamaṃ. ",
          "en": " "
        },
        {
          "scid": "sn14.29:3.7",
          "pli": "Kammapathavaggo tatiyo. ",
          "en": ""
        }
      ],
