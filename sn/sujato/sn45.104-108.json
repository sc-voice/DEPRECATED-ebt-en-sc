    {
      "segments": [
        {
          "scid": "sn45.104-108:1.1",
          "pli": "“Seyyathāpi, bhikkhave, yamunā nadī pācīnaninnā pācīnapoṇā pācīnapabbhārā; ",
          "en": "“Mendicants, the Yamunā river slants, slopes, and inclines to the east. …” "
        },
        {
          "scid": "sn45.104-108:1.2",
          "pli": "evameva kho, bhikkhave, bhikkhu … pe … ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:1.3",
          "pli": "dutiyaṃ. ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:2.1",
          "pli": "“Seyyathāpi, bhikkhave, aciravatī nadī pācīnaninnā pācīnapoṇā pācīnapabbhārā; ",
          "en": "“… the Aciravatī river …” "
        },
        {
          "scid": "sn45.104-108:2.2",
          "pli": "evameva kho, bhikkhave, bhikkhu … pe … ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:2.3",
          "pli": "tatiyaṃ. ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:3.1",
          "pli": "“Seyyathāpi, bhikkhave, sarabhū nadī pācīnaninnā pācīnapoṇā pācīnapabbhārā; ",
          "en": "“… the Sarabhū river …” "
        },
        {
          "scid": "sn45.104-108:3.2",
          "pli": "evameva kho, bhikkhave, bhikkhu … pe … ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:3.3",
          "pli": "catutthaṃ. ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:4.1",
          "pli": "“Seyyathāpi, bhikkhave, mahī nadī pācīnaninnā pācīnapoṇā pācīnapabbhārā; ",
          "en": "“… the Mahī river …” "
        },
        {
          "scid": "sn45.104-108:4.2",
          "pli": "evameva kho, bhikkhave, bhikkhu … pe … ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:4.3",
          "pli": "pañcamaṃ. ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:5.1",
          "pli": "“Seyyathāpi, bhikkhave, yā kācimā mahānadiyo, seyyathidaṃ—",
          "en": "“… all the great rivers …” "
        },
        {
          "scid": "sn45.104-108:5.2",
          "pli": "gaṅgā, yamunā, aciravatī, sarabhū, mahī, sabbā tā pācīnaninnā pācīnapoṇā pācīnapabbhārā; ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:5.3",
          "pli": "evameva kho, bhikkhave, bhikkhu … pe … ",
          "en": " "
        },
        {
          "scid": "sn45.104-108:5.4",
          "pli": "chaṭṭhaṃ. ",
          "en": ""
        }
      ],
