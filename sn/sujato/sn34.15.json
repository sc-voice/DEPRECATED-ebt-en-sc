    {
      "segments": [
        {
          "scid": "sn34.15:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn34.15:1.2",
          "pli": "“Cattārome, bhikkhave, jhāyī. ",
          "en": "“Mendicants, there are these four meditators. "
        },
        {
          "scid": "sn34.15:1.3",
          "pli": "Katame cattāro? ",
          "en": "What four? "
        },
        {
          "scid": "sn34.15:1.4",
          "pli": "Idha, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo hoti, na samādhismiṃ gocarakusalo. ",
          "en": "One meditator is skilled in entering immersion but not in the mindfulness meditation subjects for immersion. …” "
        },
        {
          "scid": "sn34.15:1.5",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ gocarakusalo hoti, na samādhismiṃ samāpattikusalo. ",
          "en": " "
        },
        {
          "scid": "sn34.15:1.6",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī neva samādhismiṃ samāpattikusalo hoti, na ca samādhismiṃ gocarakusalo. ",
          "en": " "
        },
        {
          "scid": "sn34.15:1.7",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo ca hoti, samādhismiṃ gocarakusalo ca. ",
          "en": " "
        },
        {
          "scid": "sn34.15:1.8",
          "pli": "Tatra … pe … ",
          "en": " "
        },
        {
          "scid": "sn34.15:1.9",
          "pli": "pavaro cā”ti. ",
          "en": " "
        },
        {
          "scid": "sn34.15:1.10",
          "pli": "Pannarasamaṃ. ",
          "en": ""
        }
      ],
