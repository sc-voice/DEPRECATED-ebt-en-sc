        {
          "scid": "sn35.77:1.1",
          "pli": "“Yaṃ kho, rādha, dukkhaṃ tatra te chando pahātabbo. ",
          "en": "“Rādha, you should give up desire for what is suffering. …” "
        },
        {
          "scid": "sn35.77:1.2",
          "pli": "Kiñca, rādha, dukkhaṃ? ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.3",
          "pli": "Cakkhu kho, rādha, dukkhaṃ. Tatra te chando pahātabbo. ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.4",
          "pli": "Rūpā … ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.5",
          "pli": "cakkhuviññāṇaṃ … ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.6",
          "pli": "cakkhusamphasso … ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.7",
          "pli": "yampidaṃ cakkhusamphassa … pe … adukkhamasukhaṃ vā tampi dukkhaṃ. Tatra te chando pahātabbo … pe … ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.8",
          "pli": "mano dukkho … ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.9",
          "pli": "dhammā … ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.10",
          "pli": "manoviññāṇaṃ … ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.11",
          "pli": "manosamphasso … ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.12",
          "pli": "yampidaṃ manosamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā adukkhamasukhaṃ vā tampi dukkhaṃ. Tatra te chando pahātabbo. ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.13",
          "pli": "Yaṃ kho, rādha, dukkhaṃ tatra te chando pahātabbo”ti. ",
          "en": " "
        },
        {
          "scid": "sn35.77:1.14",
          "pli": "Catutthaṃ. ",
          "en": ""
        }
      ],
