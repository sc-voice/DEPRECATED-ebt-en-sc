    },
    {
      "segments": [
        {
          "scid": "sn35.227:1.1",
          "pli": "“Rūpā, bhikkhave, anattā. Saddā … gandhā … rasā … phoṭṭhabbā … dhammā anattā. ",
          "en": "“Mendicants, sights, sounds, smells, tastes, touches, and thoughts are not-self. "
        },
        {
          "scid": "sn35.227:1.2",
          "pli": "Evaṃ passaṃ … pe … ",
          "en": "Seeing this … "
        },
        {
          "scid": "sn35.227:1.3",
          "pli": "nāparaṃ itthattāyāti pajānātī”ti. ",
          "en": "They understand: ‘… there is no return to any state of existence.’” "
        },
        {
          "scid": "sn35.227:1.4",
          "pli": "Saṭṭhipeyyālo samatto. ",
          "en": " "
        },
        {
          "scid": "sn35.227:2.1",
          "pli": "Chandenaṭṭhārasa honti, ",
          "en": " "
        },
        {
          "scid": "sn35.227:2.2",
          "pli": "atītena ca dve nava; ",
          "en": " "
        },
        {
          "scid": "sn35.227:2.3",
          "pli": "Yadaniccāṭṭhārasa vuttā, ",
          "en": " "
        },
        {
          "scid": "sn35.227:2.4",
          "pli": "tayo ajjhattabāhirā; ",
          "en": " "
        },
        {
          "scid": "sn35.227:2.5",
          "pli": "Peyyālo saṭṭhiko vutto, ",
          "en": " "
        },
        {
          "scid": "sn35.227:2.6",
          "pli": "buddhenādiccabandhunāti. ",
          "en": " "
        },
        {
          "scid": "sn35.227:2.7",
          "pli": "Suttantāni saṭṭhi. ",
          "en": ""
        }
      ],
