    {
      "segments": [
        {
          "scid": "sn34.14:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn34.14:1.2",
          "pli": "“Cattārome, bhikkhave, jhāyī. ",
          "en": "“Mendicants, there are these four meditators. "
        },
        {
          "scid": "sn34.14:1.3",
          "pli": "Katame cattāro? ",
          "en": "What four? "
        },
        {
          "scid": "sn34.14:1.4",
          "pli": "Idha, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo hoti, na samādhismiṃ ārammaṇakusalo. ",
          "en": "One meditator is skilled in entering immersion but not in the supports for it. …” "
        },
        {
          "scid": "sn34.14:1.5",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ ārammaṇakusalo hoti, na samādhismiṃ samāpattikusalo. ",
          "en": " "
        },
        {
          "scid": "sn34.14:1.6",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī neva samādhismiṃ samāpattikusalo hoti, na ca samādhismiṃ ārammaṇakusalo. ",
          "en": " "
        },
        {
          "scid": "sn34.14:1.7",
          "pli": "Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo ca hoti, samādhismiṃ ārammaṇakusalo ca. ",
          "en": " "
        },
        {
          "scid": "sn34.14:1.8",
          "pli": "Tatra … pe … ",
          "en": " "
        },
        {
          "scid": "sn34.14:1.9",
          "pli": "pavaro cā”ti. ",
          "en": " "
        },
        {
          "scid": "sn34.14:1.10",
          "pli": "Cuddasamaṃ. ",
          "en": ""
        }
      ],
