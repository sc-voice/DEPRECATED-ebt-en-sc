        {
          "scid": "sn33.16-20:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn33.16-20:1.2",
          "pli": "Ekamantaṃ nisinno kho vacchagotto paribbājako bhagavantaṃ etadavoca—",
          "en": " "
        },
        {
          "scid": "sn33.16-20:1.3",
          "pli": "ko nu kho, bho gotama, hetu, ko paccayo … pe … ",
          "en": " "
        },
        {
          "scid": "sn33.16-20:1.4",
          "pli": "rūpe kho, vaccha, ananubodhā … pe … “rūpanirodhagāminiyā paṭipadāya ananubodhā … pe …. ",
          "en": "“Vaccha, it is because of not understanding form … "
        },
        {
          "scid": "sn33.16-20:2.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": " "
        },
        {
          "scid": "sn33.16-20:2.2",
          "pli": "“Vedanāya kho, vaccha … pe …. ",
          "en": "feeling … "
        },
        {
          "scid": "sn33.16-20:3.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": " "
        },
        {
          "scid": "sn33.16-20:3.2",
          "pli": "“Saññāya kho, vaccha … pe …. ",
          "en": "perception … "
        },
        {
          "scid": "sn33.16-20:4.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": " "
        },
        {
          "scid": "sn33.16-20:4.2",
          "pli": "“Saṅkhāresu kho, vaccha … pe …. ",
          "en": "choices … "
        },
        {
          "scid": "sn33.16-20:5.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": " "
        },
        {
          "scid": "sn33.16-20:5.2",
          "pli": "“Viññāṇe kho, vaccha, ananubodhā … pe … ",
          "en": "consciousness …” "
        },
        {
          "scid": "sn33.16-20:5.3",
          "pli": "viññāṇanirodhagāminiyā paṭipadāya ananubodhā … pe … ",
          "en": " "
        },
        {
          "scid": "sn33.16-20:5.4",
          "pli": "vīsatimaṃ. ",
          "en": ""
        }
      ],
