        {
          "scid": "sn33.11-15:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn33.11-15:1.2",
          "pli": "“Rūpe kho, vaccha, anabhisamayā … pe … rūpanirodhagāminiyā paṭipadāya anabhisamayā … pe …. ",
          "en": "“Vaccha, it is because of not comprehending form … "
        },
        {
          "scid": "sn33.11-15:2.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": " "
        },
        {
          "scid": "sn33.11-15:2.2",
          "pli": "“Vedanāya kho, vaccha, anabhisamayā … pe …. ",
          "en": "feeling … "
        },
        {
          "scid": "sn33.11-15:3.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": " "
        },
        {
          "scid": "sn33.11-15:3.2",
          "pli": "“Saññāya kho, vaccha, anabhisamayā … pe …. ",
          "en": "perception … "
        },
        {
          "scid": "sn33.11-15:4.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": " "
        },
        {
          "scid": "sn33.11-15:4.2",
          "pli": "“Saṅkhāresu kho, vaccha, anabhisamayā … pe …. ",
          "en": "choices … "
        },
        {
          "scid": "sn33.11-15:5.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": " "
        },
        {
          "scid": "sn33.11-15:5.2",
          "pli": "“Viññāṇe kho, vaccha, anabhisamayā … pe …. ",
          "en": "consciousness …” "
        },
        {
          "scid": "sn33.11-15:5.3",
          "pli": "Pannarasamaṃ. ",
          "en": ""
        }
      ],
