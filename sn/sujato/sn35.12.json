      "segments": [
        {
          "scid": "sn35.12:1.1",
          "pli": "“Rūpā, bhikkhave, anattā atītānāgatā; ",
          "en": "“Mendicants, sights of the past and future are not-self, "
        },
        {
          "scid": "sn35.12:1.2",
          "pli": "ko pana vādo paccuppannānaṃ. ",
          "en": "let alone the present. …” "
        },
        {
          "scid": "sn35.12:1.3",
          "pli": "Evaṃ passaṃ, bhikkhave, sutavā ariyasāvako atītesu rūpesu anapekkho hoti; ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.4",
          "pli": "anāgate rūpe nābhinandati; ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.5",
          "pli": "paccuppannānaṃ rūpānaṃ nibbidāya virāgāya nirodhāya paṭipanno hoti. ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.6",
          "pli": "Saddā … ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.7",
          "pli": "gandhā … ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.8",
          "pli": "rasā … ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.9",
          "pli": "phoṭṭhabbā … ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.10",
          "pli": "dhammā anattā atītānāgatā; ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.11",
          "pli": "ko pana vādo paccuppannānaṃ. ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.12",
          "pli": "Evaṃ passaṃ, bhikkhave, sutavā ariyasāvako atītesu dhammesu anapekkho hoti; ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.13",
          "pli": "anāgate dhamme nābhinandati; ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.14",
          "pli": "paccuppannānaṃ dhammānaṃ nibbidāya virāgāya nirodhāya paṭipanno hotī”ti. ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.15",
          "pli": "Dvādasamaṃ. ",
          "en": " "
        },
        {
          "scid": "sn35.12:1.16",
          "pli": "Aniccavaggo paṭhamo. ",
          "en": " "
        },
        {
          "scid": "sn35.12:2.1",
          "pli": "Aniccaṃ dukkhaṃ anattā ca, ",
          "en": " "
        },
        {
          "scid": "sn35.12:2.2",
          "pli": "tayo ajjhattabāhirā; ",
          "en": " "
        },
        {
          "scid": "sn35.12:2.3",
          "pli": "Yadaniccena tayo vuttā, ",
          "en": " "
        },
        {
          "scid": "sn35.12:2.4",
          "pli": "te te ajjhattabāhirāti. ",
          "en": ""
        }
      ],
