    {
      "segments": [
        {
          "scid": "sn35.216-218:1.1",
          "pli": "“Rūpā, bhikkhave, dukkhā atītā anāgatā paccuppannā. ",
          "en": "“Mendicants, in the past … future … present sights, sounds, smells, tastes, touches, and thoughts are suffering. "
        },
        {
          "scid": "sn35.216-218:1.2",
          "pli": "Yaṃ dukkhaṃ, tadanattā. ",
          "en": "What’s suffering is not-self …” "
        },
        {
          "scid": "sn35.216-218:1.3",
          "pli": "Yadanattā, taṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ yathābhūtaṃ sammappaññāya daṭṭhabbaṃ. ",
          "en": " "
        },
        {
          "scid": "sn35.216-218:1.4",
          "pli": "Saddā … ",
          "en": " "
        },
        {
          "scid": "sn35.216-218:1.5",
          "pli": "gandhā … ",
          "en": " "
        },
        {
          "scid": "sn35.216-218:1.6",
          "pli": "rasā … ",
          "en": " "
        },
        {
          "scid": "sn35.216-218:1.7",
          "pli": "phoṭṭhabbā … ",
          "en": " "
        },
        {
          "scid": "sn35.216-218:1.8",
          "pli": "dhammā dukkhā atītā anāgatā paccuppannā. ",
          "en": " "
        },
        {
          "scid": "sn35.216-218:1.9",
          "pli": "Yaṃ dukkhaṃ, tadanattā. ",
          "en": " "
        },
        {
          "scid": "sn35.216-218:1.10",
          "pli": "Yadanattā, taṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ yathābhūtaṃ sammappaññāya daṭṭhabbaṃ. ",
          "en": " "
        },
        {
          "scid": "sn35.216-218:1.11",
          "pli": "Evaṃ passaṃ … pe … ",
          "en": " "
        },
        {
          "scid": "sn35.216-218:1.12",
          "pli": "nāparaṃ itthattāyāti pajānātī”ti. ",
          "en": ""
        }
      ],
