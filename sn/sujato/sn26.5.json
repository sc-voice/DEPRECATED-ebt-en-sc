    {
      "segments": [
        {
          "scid": "sn26.5:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn26.5:1.2",
          "pli": "“Yo kho, bhikkhave, cakkhusamphassajāya vedanāya uppādo ṭhiti … pe … jarāmaraṇassa pātubhāvo … pe …. ",
          "en": "“Mendicants, the arising of feeling born of eye contact … "
        },
        {
          "scid": "sn26.5:2.1",
          "pli": "Yo manosamphassajāya vedanāya uppādo ṭhiti … pe … ",
          "en": "the arising of feeling born of mind contact … "
        },
        {
          "scid": "sn26.5:2.2",
          "pli": "jarāmaraṇassa pātubhāvo. ",
          "en": "is the manifestation of old age and death. "
        },
        {
          "scid": "sn26.5:2.3",
          "pli": "Yo ca kho, bhikkhave, cakkhusamphassajāya vedanāya nirodho vūpasamo … pe … ",
          "en": "The cessation of feeling born of eye contact … "
        },
        {
          "scid": "sn26.5:2.4",
          "pli": "jarāmaraṇassa atthaṅgamo … pe … ",
          "en": " "
        },
        {
          "scid": "sn26.5:2.5",
          "pli": "yo manosamphassajāya vedanāya nirodho vūpasamo atthaṅgamo, dukkhasseso nirodho, rogānaṃ vūpasamo, jarāmaraṇassa atthaṅgamo”ti. ",
          "en": "the cessation of feeling born of mind contact … is the ending of old age and death.” "
        },
        {
          "scid": "sn26.5:2.6",
          "pli": "Pañcamaṃ. ",
          "en": ""
        }
      ],
