    },
    {
      "segments": [
        {
          "scid": "sn12.83-92:1.1",
          "pli": "“Jātiṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ … pe …. (83:354) ",
          "en": "“Mendicants, one who does not truly know or see rebirth … "
        },
        {
          "scid": "sn12.83-92:2.1",
          "pli": "“Bhavaṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ … pe …. (84:355) ",
          "en": "continued existence … "
        },
        {
          "scid": "sn12.83-92:3.1",
          "pli": "“Upādānaṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ … pe …. (85:356) ",
          "en": "grasping … "
        },
        {
          "scid": "sn12.83-92:4.1",
          "pli": "“Taṇhaṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ … pe …. (86:357) ",
          "en": "craving … "
        },
        {
          "scid": "sn12.83-92:5.1",
          "pli": "“Vedanaṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ … pe …. (87:358) ",
          "en": "feeling … "
        },
        {
          "scid": "sn12.83-92:6.1",
          "pli": "“Phassaṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ … pe …. (88:359) ",
          "en": "contact … "
        },
        {
          "scid": "sn12.83-92:7.1",
          "pli": "“Saḷāyatanaṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ … pe …. (89:360) ",
          "en": "the six sense fields … "
        },
        {
          "scid": "sn12.83-92:8.1",
          "pli": "“Nāmarūpaṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ … pe …. (90:361) ",
          "en": "name and form … "
        },
        {
          "scid": "sn12.83-92:9.1",
          "pli": "“Viññāṇaṃ, bhikkhave, ajānatā apassatā yathābhūtaṃ … pe …. (91:362) ",
          "en": "consciousness … "
        },
        {
          "scid": "sn12.83-92:10.1",
          "pli": "“Saṅkhāre, bhikkhave, ajānatā apassatā yathābhūtaṃ saṅkhāresu yathābhūtaṃ ñāṇāya satthā pariyesitabbo; ",
          "en": "choices …” "
        },
        {
          "scid": "sn12.83-92:10.2",
          "pli": "saṅkhārasamudayaṃ ajānatā apassatā yathābhūtaṃ saṅkhārasamudaye yathābhūtaṃ ñāṇāya satthā pariyesitabbo; ",
          "en": " "
        },
        {
          "scid": "sn12.83-92:10.3",
          "pli": "saṅkhāranirodhaṃ ajānatā apassatā yathābhūtaṃ saṅkhāranirodhe yathābhūtaṃ ñāṇāya satthā pariyesitabbo; ",
          "en": " "
        },
        {
          "scid": "sn12.83-92:10.4",
          "pli": "saṅkhāranirodhagāminiṃ paṭipadaṃ ajānatā apassatā yathābhūtaṃ saṅkhāranirodhagāminiyā paṭipadāya yathābhūtaṃ ñāṇāya satthā pariyesitabbo”ti. (92:363) ",
          "en": " "
        },
        {
          "scid": "sn12.83-92:10.5",
          "pli": "Ekādasamaṃ. ",
          "en": " "
        },
        {
          "scid": "sn12.83-92:10.6",
          "pli": "(Sabbesaṃ catusaccikaṃ kātabbaṃ.) ",
          "en": "(All should be treated according to the four truths.)"
        }
      ],
