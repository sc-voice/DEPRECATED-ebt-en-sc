      "segments": [
        {
          "scid": "sn26.4:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn26.4:1.2",
          "pli": "“Yo kho, bhikkhave, cakkhusamphassassa uppādo ṭhiti … pe … ",
          "en": "“Mendicants, the arising of eye contact … "
        },
        {
          "scid": "sn26.4:1.3",
          "pli": "jarāmaraṇassa pātubhāvo … pe … ",
          "en": " "
        },
        {
          "scid": "sn26.4:1.4",
          "pli": "yo manosamphassassa uppādo ṭhiti … pe … ",
          "en": "mind contact … "
        },
        {
          "scid": "sn26.4:1.5",
          "pli": "jarāmaraṇassa pātubhāvo. ",
          "en": "is the manifestation of old age and death. "
        },
        {
          "scid": "sn26.4:1.6",
          "pli": "Yo ca kho, bhikkhave, cakkhusamphassassa nirodho … pe … ",
          "en": "The cessation of eye contact … "
        },
        {
          "scid": "sn26.4:1.7",
          "pli": "jarāmaraṇassa atthaṅgamo … pe … ",
          "en": " "
        },
        {
          "scid": "sn26.4:1.8",
          "pli": "yo manosamphassassa nirodho … pe … ",
          "en": "mind contact … "
        },
        {
          "scid": "sn26.4:1.9",
          "pli": "jarāmaraṇassa atthaṅgamo”ti. ",
          "en": "is the ending of old age and death.” "
        },
        {
          "scid": "sn26.4:1.10",
          "pli": "Catutthaṃ. ",
          "en": ""
        }
      ],
