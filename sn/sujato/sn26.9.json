    {
      "segments": [
        {
          "scid": "sn26.9:1.1",
          "pli": "Sāvatthinidānaṃ. ",
          "en": "At Sāvatthī. "
        },
        {
          "scid": "sn26.9:1.2",
          "pli": "“Yo kho, bhikkhave, pathavīdhātuyā uppādo ṭhiti abhinibbatti pātubhāvo … pe … jarāmaraṇassa pātubhāvo; ",
          "en": "“Mendicants, the arising, continuation, rebirth, and manifestation of the earth element, "
        },
        {
          "scid": "sn26.9:1.3",
          "pli": "yo āpodhātuyā … ",
          "en": "the water element, "
        },
        {
          "scid": "sn26.9:1.4",
          "pli": "yo tejodhātuyā … ",
          "en": "the fire element, "
        },
        {
          "scid": "sn26.9:1.5",
          "pli": "yo vāyodhātuyā … ",
          "en": "the air element, "
        },
        {
          "scid": "sn26.9:1.6",
          "pli": "yo ākāsadhātuyā … ",
          "en": "the space element, "
        },
        {
          "scid": "sn26.9:1.7",
          "pli": "yo viññāṇadhātuyā uppādo ṭhiti abhinibbatti pātubhāvo, dukkhasseso uppādo, rogānaṃ ṭhiti, jarāmaraṇassa pātubhāvo. ",
          "en": "and the consciousness element is the arising of suffering, the continuation of diseases, and the manifestation of old age and death. "
        },
        {
          "scid": "sn26.9:1.8",
          "pli": "Yo ca kho, bhikkhave, pathavīdhātuyā nirodho … pe … jarāmaraṇassa atthaṅgamo; ",
          "en": "The cessation of the earth element, "
        },
        {
          "scid": "sn26.9:1.9",
          "pli": "yo āpodhātuyā nirodho … ",
          "en": "the water element, "
        },
        {
          "scid": "sn26.9:1.10",
          "pli": "yo tejodhātuyā nirodho … ",
          "en": "the fire element, "
        },
        {
          "scid": "sn26.9:1.11",
          "pli": "yo vāyodhātuyā nirodho … ",
          "en": "the air element, "
        },
        {
          "scid": "sn26.9:1.12",
          "pli": "yo ākāsadhātuyā nirodho … ",
          "en": "the space element, "
        },
        {
          "scid": "sn26.9:1.13",
          "pli": "yo viññāṇadhātuyā nirodho vūpasamo atthaṅgamo, dukkhasseso nirodho, rogānaṃ vūpasamo, jarāmaraṇassa atthaṅgamo”ti. ",
          "en": "and the consciousness element is the cessation of suffering, the settling of diseases, and the ending of old age and death.” "
        },
        {
          "scid": "sn26.9:1.14",
          "pli": "Navamaṃ. ",
          "en": ""
        }
      ],
