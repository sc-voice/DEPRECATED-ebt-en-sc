    {
      "segments": [
        {
          "scid": "sn50.89-98:1.1",
          "pli": "(Evaṃ esanāpāḷi vitthāretabbā—rāgavinayapariyosānaṃ dosavinayapariyosānaṃ mohavinayapariyosānaṃ.) ",
          "en": "(To be expanded as in SN 45.161–170, removal of greed version.) "
        },
        {
          "scid": "sn50.89-98:1.2",
          "pli": "Esanāvaggo navamo. ",
          "en": " "
        },
        {
          "scid": "sn50.89-98:2.1",
          "pli": "Esanā vidhā āsavo, ",
          "en": "Searches, discriminations, defilements, "
        },
        {
          "scid": "sn50.89-98:2.2",
          "pli": "Bhavo ca dukkhatā tisso; ",
          "en": "states of existence, three kinds of suffering, "
        },
        {
          "scid": "sn50.89-98:2.3",
          "pli": "Khilaṃ malañca nīgho ca, ",
          "en": "barrenness, stains, and troubles, "
        },
        {
          "scid": "sn50.89-98:2.4",
          "pli": "Vedanā taṇhā tasinā cāti. ",
          "en": "feelings, craving, and thirst."
        }
      ],
