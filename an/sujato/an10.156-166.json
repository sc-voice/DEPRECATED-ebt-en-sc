    },
    {
      "segments": [
        {
          "scid": "an10.156-166:1.1",
          "pli": "“Dasahi, bhikkhave, dhammehi samannāgato puggalo na bhajitabbo … pe … ",
          "en": "“Mendicants, you should not frequent a person who has ten qualities. … "
        },
        {
          "scid": "an10.156-166:1.2",
          "pli": "bhajitabbo … pe … ",
          "en": "you should frequent … "
        },
        {
          "scid": "an10.156-166:1.3",
          "pli": "na payirupāsitabbo … ",
          "en": "you should not pay homage … "
        },
        {
          "scid": "an10.156-166:1.4",
          "pli": "payirupāsitabbo … pe … ",
          "en": "you should pay homage … "
        },
        {
          "scid": "an10.156-166:1.5",
          "pli": "na pujjo hoti … ",
          "en": "you should not venerate … "
        },
        {
          "scid": "an10.156-166:1.6",
          "pli": "pujjo hoti … pe … ",
          "en": "you should venerate … "
        },
        {
          "scid": "an10.156-166:1.7",
          "pli": "na pāsaṃso hoti … ",
          "en": "you should not praise … "
        },
        {
          "scid": "an10.156-166:1.8",
          "pli": "pāsaṃso hoti … pe … ",
          "en": "you should praise … "
        },
        {
          "scid": "an10.156-166:1.9",
          "pli": "agāravo hoti … ",
          "en": "you should not respect … "
        },
        {
          "scid": "an10.156-166:1.10",
          "pli": "sagāravo hoti … pe … ",
          "en": "you should respect … "
        },
        {
          "scid": "an10.156-166:1.11",
          "pli": "appatisso hoti … ",
          "en": "you should not revere … "
        },
        {
          "scid": "an10.156-166:1.12",
          "pli": "sappatisso hoti … pe … ",
          "en": "you should revere … "
        },
        {
          "scid": "an10.156-166:1.13",
          "pli": "na ārādhako hoti … ",
          "en": "is not a success … "
        },
        {
          "scid": "an10.156-166:1.14",
          "pli": "ārādhako hoti … pe … ",
          "en": "is a success … "
        },
        {
          "scid": "an10.156-166:1.15",
          "pli": "na visujjhati … ",
          "en": "is not pure … "
        },
        {
          "scid": "an10.156-166:1.16",
          "pli": "visujjhati … pe … ",
          "en": "is pure … "
        },
        {
          "scid": "an10.156-166:1.17",
          "pli": "mānaṃ nādhibhoti … ",
          "en": "does not win over conceit … "
        },
        {
          "scid": "an10.156-166:1.18",
          "pli": "mānaṃ adhibhoti … pe … ",
          "en": "wins over conceit … "
        },
        {
          "scid": "an10.156-166:1.19",
          "pli": "paññāya na vaḍḍhati … ",
          "en": "does not grow in wisdom … "
        },
        {
          "scid": "an10.156-166:1.20",
          "pli": "paññāya vaḍḍhati … pe …. ",
          "en": "grows in wisdom … "
        },
        {
          "scid": "an10.156-166:2.1",
          "pli": "Bahuṃ apuññaṃ pasavati … ",
          "en": "makes much bad karma … "
        },
        {
          "scid": "an10.156-166:2.2",
          "pli": "bahuṃ puññaṃ pasavati. ",
          "en": "makes much merit. "
        },
        {
          "scid": "an10.156-166:2.3",
          "pli": "Katamehi dasahi? ",
          "en": "What ten? "
        },
        {
          "scid": "an10.156-166:2.4",
          "pli": "Sammādiṭṭhiko hoti, sammāsaṅkappo hoti, sammāvāco hoti, sammākammanto hoti, sammāājīvo hoti, sammāvāyāmo hoti, sammāsati hoti, sammāsamādhi hoti, sammāñāṇī hoti, sammāvimutti hoti—",
          "en": "Right view, right thought, right speech, right action, right livelihood, right effort, right mindfulness, right immersion, right knowledge, and right freedom. "
        },
        {
          "scid": "an10.156-166:2.5",
          "pli": "imehi kho, bhikkhave, dasahi dhammehi samannāgato puggalo bahuṃ puññaṃ pasavatī”ti. ",
          "en": "A person who has these ten qualities makes much merit.” "
        },
        {
          "scid": "an10.156-166:2.6",
          "pli": "Puggalavaggo paṭhamo. ",
          "en": ""
        }
      ],
