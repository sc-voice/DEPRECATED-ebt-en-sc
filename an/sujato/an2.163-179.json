    {
      "segments": [
        {
          "scid": "an2.163-179:163.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:163.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:163.1.3",
          "pli": "Samāpattikusalatā ca samāpattivuṭṭhānakusalatā ca. ",
          "en": "Skill in meditative attainments and skill in emerging from those attainments. "
        },
        {
          "scid": "an2.163-179:163.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:163.1.5",
          "pli": "164 ",
          "en": "164 "
        },
        {
          "scid": "an2.163-179:164.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:164.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:164.1.3",
          "pli": "Ajjavañca maddavañca. ",
          "en": "Integrity and gentleness. "
        },
        {
          "scid": "an2.163-179:164.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:164.1.5",
          "pli": "165 ",
          "en": "165 "
        },
        {
          "scid": "an2.163-179:165.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:165.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:165.1.3",
          "pli": "Khanti ca soraccañca. ",
          "en": "Patience and gentleness. "
        },
        {
          "scid": "an2.163-179:165.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:165.1.5",
          "pli": "166 ",
          "en": "166 "
        },
        {
          "scid": "an2.163-179:166.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:166.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:166.1.3",
          "pli": "Sākhalyañca paṭisanthāro ca. ",
          "en": "Friendliness and hospitality. "
        },
        {
          "scid": "an2.163-179:166.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:166.1.5",
          "pli": "167 ",
          "en": "167 "
        },
        {
          "scid": "an2.163-179:167.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:167.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:167.1.3",
          "pli": "Avihiṃsā ca soceyyañca. ",
          "en": "Harmlessness and purity. "
        },
        {
          "scid": "an2.163-179:167.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:167.1.5",
          "pli": "168 ",
          "en": "168 "
        },
        {
          "scid": "an2.163-179:168.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:168.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:168.1.3",
          "pli": "Indriyesu aguttadvāratā ca bhojane amattaññutā ca. ",
          "en": "Not guarding the sense doors and eating too much. "
        },
        {
          "scid": "an2.163-179:168.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:168.1.5",
          "pli": "169 ",
          "en": "169 "
        },
        {
          "scid": "an2.163-179:169.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:169.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:169.1.3",
          "pli": "Indriyesu guttadvāratā ca bhojane mattaññutā ca. ",
          "en": "Guarding the sense doors and moderation in eating. "
        },
        {
          "scid": "an2.163-179:169.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:169.1.5",
          "pli": "170 ",
          "en": "170 "
        },
        {
          "scid": "an2.163-179:170.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:170.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:170.1.3",
          "pli": "Paṭisaṅkhānabalañca bhāvanābalañca. ",
          "en": "The power of reflection and the power of development. "
        },
        {
          "scid": "an2.163-179:170.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:170.1.5",
          "pli": "171 ",
          "en": "171 "
        },
        {
          "scid": "an2.163-179:171.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:171.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:171.1.3",
          "pli": "Satibalañca samādhibalañca. ",
          "en": "The power of mindfulness and the power of immersion. "
        },
        {
          "scid": "an2.163-179:171.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:171.1.5",
          "pli": "172 ",
          "en": "172 "
        },
        {
          "scid": "an2.163-179:172.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:172.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:172.1.3",
          "pli": "Samatho ca vipassanā ca. ",
          "en": "Serenity and discernment. "
        },
        {
          "scid": "an2.163-179:172.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:172.1.5",
          "pli": "173 ",
          "en": "173 "
        },
        {
          "scid": "an2.163-179:173.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:173.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:173.1.3",
          "pli": "Sīlavipatti ca diṭṭhivipatti ca. ",
          "en": "Failure in ethical conduct and failure in view. "
        },
        {
          "scid": "an2.163-179:173.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:173.1.5",
          "pli": "174 ",
          "en": "174 "
        },
        {
          "scid": "an2.163-179:174.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:174.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:174.1.3",
          "pli": "Sīlasampadā ca diṭṭhisampadā ca. ",
          "en": "Accomplishment in ethical conduct and accomplishment in view. "
        },
        {
          "scid": "an2.163-179:174.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:174.1.5",
          "pli": "175 ",
          "en": "175 "
        },
        {
          "scid": "an2.163-179:175.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:175.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:175.1.3",
          "pli": "Sīlavisuddhi ca diṭṭhivisuddhi ca. ",
          "en": "Purification of ethics and purification of view. "
        },
        {
          "scid": "an2.163-179:175.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:175.1.5",
          "pli": "176 ",
          "en": "176 "
        },
        {
          "scid": "an2.163-179:176.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:176.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:176.1.3",
          "pli": "Diṭṭhivisuddhi ca yathādiṭṭhissa ca padhānaṃ. ",
          "en": "Purification of view and making an effort in line with that view. "
        },
        {
          "scid": "an2.163-179:176.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:176.1.5",
          "pli": "177 ",
          "en": "177 "
        },
        {
          "scid": "an2.163-179:177.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:177.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:177.1.3",
          "pli": "Asantuṭṭhitā ca kusalesu dhammesu, appaṭivānitā ca padhānasmiṃ. ",
          "en": "To never be content with skillful qualities, and to never stop trying. "
        },
        {
          "scid": "an2.163-179:177.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:177.1.5",
          "pli": "178 ",
          "en": "178 "
        },
        {
          "scid": "an2.163-179:178.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:178.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:178.1.3",
          "pli": "Muṭṭhassaccañca asampajaññañca. ",
          "en": "Lack of mindfulness and lack of situational awareness. "
        },
        {
          "scid": "an2.163-179:178.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:178.1.5",
          "pli": "179 ",
          "en": "179 "
        },
        {
          "scid": "an2.163-179:179.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are these two things. "
        },
        {
          "scid": "an2.163-179:179.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.163-179:179.1.3",
          "pli": "Sati ca sampajaññañca. ",
          "en": "Mindfulness and situational awareness. "
        },
        {
          "scid": "an2.163-179:179.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.163-179:179.1.5",
          "pli": "Samāpattivaggo pañcamo. ",
          "en": " "
        },
        {
          "scid": "an2.163-179:179.1.6",
          "pli": "Tatiyo paṇṇāsako samatto. ",
          "en": ""
        }
      ],
