        {
          "scid": "an7.96-614:1.1",
          "pli": "“Sattime, bhikkhave, puggalā āhuneyyā pāhuneyyā … pe … anuttaraṃ puññakkhettaṃ lokassa. ",
          "en": "“Mendicants, these seven people are worthy of offerings … "
        },
        {
          "scid": "an7.96-614:1.2",
          "pli": "Katame satta? ",
          "en": "What seven? "
        },
        {
          "scid": "an7.96-614:1.3",
          "pli": "Idha, bhikkhave, ekacco puggalo cakkhusmiṃ dukkhānupassī viharati … pe … ",
          "en": "First, take a person who meditates observing suffering in the eye. … "
        },
        {
          "scid": "an7.96-614:1.4",
          "pli": "cakkhusmiṃ anattānupassī viharati … pe … ",
          "en": "observing not-self in the eye. … "
        },
        {
          "scid": "an7.96-614:1.5",
          "pli": "cakkhusmiṃ khayānupassī viharati … pe … ",
          "en": "observing ending in the eye. … "
        },
        {
          "scid": "an7.96-614:1.6",
          "pli": "cakkhusmiṃ vayānupassī viharati … pe … ",
          "en": "observing vanishing in the eye. … "
        },
        {
          "scid": "an7.96-614:1.7",
          "pli": "cakkhusmiṃ virāgānupassī viharati … pe … ",
          "en": "observing fading away in the eye. … "
        },
        {
          "scid": "an7.96-614:1.8",
          "pli": "cakkhusmiṃ nirodhānupassī viharati … pe … ",
          "en": "observing cessation in the eye. … "
        },
        {
          "scid": "an7.96-614:1.9",
          "pli": "cakkhusmiṃ paṭinissaggānupassī viharati … pe …. (2–8.) ",
          "en": "observing letting go in the eye. … "
        },
