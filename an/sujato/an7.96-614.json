          "pli": "vayānupassī viharati … ",
          "en": "vanishing … "
        },
        {
          "scid": "an7.96-614:12.11",
          "pli": "virāgānupassī viharati … ",
          "en": "fading away … "
        },
        {
          "scid": "an7.96-614:12.12",
          "pli": "nirodhānupassī viharati … ",
          "en": "cessation … "
        },
        {
          "scid": "an7.96-614:12.13",
          "pli": "paṭinissaggānupassī viharati … pe … ",
          "en": "letting go …” "
        },
        {
          "scid": "an7.96-614:12.14",
          "pli": "lokassā”ti. (481–528.) ",
          "en": " "
        },
        {
          "scid": "an7.96-614:13.1",
          "pli": "“Chadvārārammaṇesvettha, ",
          "en": " "
        },
        {
          "scid": "an7.96-614:13.2",
          "pli": "viññāṇesu ca phassesu; ",
          "en": " "
        },
        {
          "scid": "an7.96-614:13.3",
          "pli": "Vedanāsu ca dvārassa, ",
          "en": " "
        },
        {
          "scid": "an7.96-614:13.4",
          "pli": "suttā honti visuṃ aṭṭha. ",
          "en": " "
        },
        {
          "scid": "an7.96-614:14.1",
          "pli": "Saññā sañcetanā taṇhā, ",
          "en": " "
        },
        {
          "scid": "an7.96-614:14.2",
          "pli": "vitakkesu vicāre ca; ",
          "en": " "
        },
        {
          "scid": "an7.96-614:14.3",
          "pli": "Gocarassa visuṃ aṭṭha, ",
          "en": " "
        },
        {
          "scid": "an7.96-614:14.4",
          "pli": "pañcakkhandhe ca pacceke. ",
          "en": " "
        },
        {
          "scid": "an7.96-614:15.1",
          "pli": "Soḷasasvettha mūlesu, ",
          "en": " "
        },
        {
          "scid": "an7.96-614:15.2",
          "pli": "aniccaṃ dukkhamanattā; ",
          "en": " "
        },
        {
          "scid": "an7.96-614:15.3",
          "pli": "Khayā vayā virāgā ca, ",
          "en": " "
        },
        {
          "scid": "an7.96-614:15.4",
          "pli": "nirodhā paṭinissaggā. ",
          "en": " "
        },
        {
          "scid": "an7.96-614:16.1",
          "pli": "Kamaṃ aṭṭhānupassanā, ",
          "en": " "
        },
        {
          "scid": "an7.96-614:16.2",
          "pli": "yojetvāna visuṃ visuṃ; ",
          "en": " "
        },
        {
          "scid": "an7.96-614:16.3",
          "pli": "Sampiṇḍitesu sabbesu, ",
          "en": " "
        },
        {
          "scid": "an7.96-614:16.4",
          "pli": "honti pañca satāni ca; ",
          "en": " "
        },
        {
          "scid": "an7.96-614:16.5",
          "pli": "Aṭṭhavīsati suttāni, ",
          "en": " "
        },
        {
          "scid": "an7.96-614:16.6",
          "pli": "āhuneyye ca vaggike”. ",
          "en": " "
        },
        {
          "scid": "an7.96-614:16.7",
          "pli": "Āhuneyyavaggo dasamo. ",
          "en": ""
        }
