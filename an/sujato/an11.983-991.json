        },
        {
          "scid": "an11.983-991:4.1",
          "pli": "AN 11.983–991 ",
          "en": "983–991 "
        },
        {
          "scid": "an11.983-991:5.1",
          "pli": "“Rāgassa, bhikkhave, pariññāya… ",
          "en": "“For the complete understanding of greed … "
        },
        {
          "scid": "an11.983-991:5.2",
          "pli": "parikkhayāya… ",
          "en": "complete ending … "
        },
        {
          "scid": "an11.983-991:5.3",
          "pli": "pahānāya… ",
          "en": "giving up … "
        },
        {
          "scid": "an11.983-991:5.4",
          "pli": "khayāya… ",
          "en": "ending … "
        },
        {
          "scid": "an11.983-991:5.5",
          "pli": "vayāya… ",
          "en": "vanishing … "
        },
        {
          "scid": "an11.983-991:5.6",
          "pli": "virāgāya… ",
          "en": "fading away … "
        },
        {
          "scid": "an11.983-991:5.7",
          "pli": "nirodhāya… ",
          "en": "cessation … "
        },
        {
          "scid": "an11.983-991:5.8",
          "pli": "cāgāya… ",
          "en": "giving away … "
        },
        {
          "scid": "an11.983-991:5.9",
          "pli": "paṭinissaggāya… ",
          "en": "letting go of negligence … "
        },
        {
          "scid": "an11.983-991:5.10",
          "pli": "ime ekādasa dhammā bhāvetabbā”. ",
          "en": "these eleven things should be developed.”"
        }
      ],
