      "segments": [
        {
          "scid": "an2.87-97:87.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:87.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:87.1.3",
          "pli": "Cetovimutti ca paññāvimutti ca. ",
          "en": "Freedom of heart and freedom by wisdom. "
        },
        {
          "scid": "an2.87-97:87.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:87.1.5",
          "pli": "88 ",
          "en": "88 "
        },
        {
          "scid": "an2.87-97:88.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:88.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:88.1.3",
          "pli": "Paggāho ca avikkhepo ca. ",
          "en": "Exertion, and not being distracted. "
        },
        {
          "scid": "an2.87-97:88.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:88.1.5",
          "pli": "89 ",
          "en": "89 "
        },
        {
          "scid": "an2.87-97:89.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:89.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:89.1.3",
          "pli": "Nāmañca rūpañca. ",
          "en": "Name and form. "
        },
        {
          "scid": "an2.87-97:89.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:89.1.5",
          "pli": "90 ",
          "en": "90 "
        },
        {
          "scid": "an2.87-97:90.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:90.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:90.1.3",
          "pli": "Vijjā ca vimutti ca. ",
          "en": "Knowledge and freedom. "
        },
        {
          "scid": "an2.87-97:90.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:90.1.5",
          "pli": "91 ",
          "en": "91 "
        },
        {
          "scid": "an2.87-97:91.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:91.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:91.1.3",
          "pli": "Bhavadiṭṭhi ca vibhavadiṭṭhi ca. ",
          "en": "Views favoring continued existence and views favoring ending existence. "
        },
        {
          "scid": "an2.87-97:91.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:91.1.5",
          "pli": "92 ",
          "en": "92 "
        },
        {
          "scid": "an2.87-97:92.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:92.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:92.1.3",
          "pli": "Ahirikañca anottappañca. ",
          "en": "Lack of conscience and prudence. "
        },
        {
          "scid": "an2.87-97:92.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:92.1.5",
          "pli": "93 ",
          "en": "93 "
        },
        {
          "scid": "an2.87-97:93.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:93.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:93.1.3",
          "pli": "Hirī ca ottappañca. ",
          "en": "Conscience and prudence. "
        },
        {
          "scid": "an2.87-97:93.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:93.1.5",
          "pli": "94 ",
          "en": "94 "
        },
        {
          "scid": "an2.87-97:94.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:94.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:94.1.3",
          "pli": "Dovacassatā ca pāpamittatā ca. ",
          "en": "Being hard to admonish and having bad friends. "
        },
        {
          "scid": "an2.87-97:94.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:94.1.5",
          "pli": "95 ",
          "en": "95 "
        },
        {
          "scid": "an2.87-97:95.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:95.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:95.1.3",
          "pli": "Sovacassatā ca kalyāṇamittatā ca. ",
          "en": "Being easy to admonish and having good friends. "
        },
        {
          "scid": "an2.87-97:95.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:95.1.5",
          "pli": "96 ",
          "en": "96 "
        },
        {
          "scid": "an2.87-97:96.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:96.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:96.1.3",
          "pli": "Dhātukusalatā ca manasikārakusalatā ca. ",
          "en": "Skill in the elements and skill in attention. "
        },
        {
          "scid": "an2.87-97:96.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:96.1.5",
          "pli": "97 ",
          "en": "97 "
        },
        {
          "scid": "an2.87-97:97.1.1",
          "pli": "“Dveme, bhikkhave, dhammā. ",
          "en": "“There are, mendicants, these two things. "
        },
        {
          "scid": "an2.87-97:97.1.2",
          "pli": "Katame dve? ",
          "en": "What two? "
        },
        {
          "scid": "an2.87-97:97.1.3",
          "pli": "Āpattikusalatā ca āpattivuṭṭhānakusalatā ca. ",
          "en": "Skill in offenses and skill in rehabilitation from offenses. "
        },
        {
          "scid": "an2.87-97:97.1.4",
          "pli": "Ime kho, bhikkhave, dve dhammā”ti. ",
          "en": "These are the two things.” "
        },
        {
          "scid": "an2.87-97:97.1.5",
          "pli": "Dhammavaggo catuttho. ",
          "en": ""
        }
