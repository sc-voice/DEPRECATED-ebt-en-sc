    {
      "segments": [
        {
          "scid": "an10.225-228:1.1",
          "pli": "“Dasahi, bhikkhave, dhammehi samannāgato khataṃ upahataṃ attānaṃ pariharati … pe … ",
          "en": "“Someone with ten qualities keeps themselves broken and damaged … "
        },
        {
          "scid": "an10.225-228:1.2",
          "pli": "akkhataṃ anupahataṃ attānaṃ pariharati … pe … ",
          "en": "keeps themselves healthy and whole … "
        },
        {
          "scid": "an10.225-228:1.3",
          "pli": "vīsatiyā, bhikkhave … pe … ",
          "en": "twenty … "
        },
        {
          "scid": "an10.225-228:1.4",
          "pli": "tiṃsāya, bhikkhave … pe … ",
          "en": "thirty … "
        },
        {
          "scid": "an10.225-228:1.5",
          "pli": "cattārīsāya, bhikkhave, dhammehi samannāgato khataṃ upahataṃ attānaṃ pariharati … pe …. ",
          "en": "forty …” "
        },
        {
          "scid": "an10.225-228:1.6",
          "pli": "() ",
          "en": "()"
        }
      ],
