      "segments": [
        {
          "scid": "an6.140:1.1",
          "pli": "“Rāgassa, bhikkhave, abhiññāya cha dhammā bhāvetabbā. ",
          "en": "“For insight into greed, six things should be developed. "
        },
        {
          "scid": "an6.140:1.2",
          "pli": "Katame cha? ",
          "en": "What six? "
        },
        {
          "scid": "an6.140:1.3",
          "pli": "Dassanānuttariyaṃ, savanānuttariyaṃ, lābhānuttariyaṃ, sikkhānuttariyaṃ, pāricariyānuttariyaṃ, anussatānuttariyaṃ. ",
          "en": "The unsurpassable seeing, listening, acquisition, training, service, and recollection. "
        },
        {
          "scid": "an6.140:1.4",
          "pli": "Rāgassa, bhikkhave, abhiññāya ime cha dhammā bhāvetabbā”ti. ",
          "en": "For insight into greed, these six things should be developed.”"
        }
      ],
